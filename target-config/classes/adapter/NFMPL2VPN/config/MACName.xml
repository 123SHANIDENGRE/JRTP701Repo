<EntityList name="NFMPFilterGroup" RootFilterExpression="$[?(@.className=='epipe.PbbMacName')]" FilterHandler="com.nokia.nsw.uiv.adapter.handler.xml.NFMPL2VPNGenericNodeHandler">
	<Entity type="Configuration" markForAsso="true">
		<Filter>
			<ValueExpression>contains(/JsonRootNode/className,'epipe.PbbMacName') 
			</ValueExpression>
		</Filter>
		<AttributeList>
			<Attribute>
				<Name>LocalName</Name>
				<ValueExpression>substring-after(/JsonRootNode/objectFullName,'network:')</ValueExpression>
				<Context>
					<ValueExpression/>
				</Context>
			</Attribute>
			<Attribute>
				<Name>Context</Name>
				<ValueExpression>substring-before(/JsonRootNode/objectFullName,':')</ValueExpression>
				<DefaultValue/>
			</Attribute>
			<Attribute>
				<Name>Kind</Name>
				<ValueExpression/>
				<DefaultValue>MACName</DefaultValue>
			</Attribute>
			<Attribute>
				<Name>Category</Name>
				<ValueExpression/>
				<DefaultValue>PbbMacName</DefaultValue>
			</Attribute>
			<Attribute>
				<Name>DiscoveredName</Name>
				<ValueExpression>/JsonRootNode/objectFullName</ValueExpression>
				<DefaultValue/>
			</Attribute>
			<Attribute>
				<Name>DisplayName</Name>
				<ValueExpression>/JsonRootNode/macName</ValueExpression>
				<DefaultValue/>
			</Attribute>
			<Attribute>
				<Name>Property</Name>
				<ValueExpression>/JsonRootNode/siteId</ValueExpression>
				<Key>siteId</Key>
			</Attribute>
			
			<Attribute>
				<Name>Property</Name>
				<!-- <ValueExpression>/JsonRootNode/deploymentState</ValueExpression> -->
				<ValueExpression>if(/JsonRootNode/deploymentState = 0) then "deployed" else /JsonRootNode/deploymentState</ValueExpression>
				<Key>deploymentState</Key>
			</Attribute>
			<Attribute>
				<Name>Property</Name>
				<ValueExpression>/JsonRootNode/selfAlarmed</ValueExpression>
				<Key>selfAlarmed</Key>
			</Attribute>
			<Attribute>
				<Name>Property</Name>
				<ValueExpression>/JsonRootNode/macNameAddress</ValueExpression>
				<Key>macNameAddress</Key>
			</Attribute>
			<Attribute>
				<Name>Property</Name>
				<ValueExpression>/JsonRootNode/macName</ValueExpression>
				<Key>macName</Key>
			</Attribute>
		</AttributeList>
		<AssoicationList>
			<KeyBasedAssoication>
				<SelfKey>
					<Key>
						<Name>MACEquipment</Name>
						<ValueExpression>/JsonRootNode/siteId</ValueExpression>
					</Key>
				</SelfKey>
			</KeyBasedAssoication>
		</AssoicationList>
	</Entity>
</EntityList>
